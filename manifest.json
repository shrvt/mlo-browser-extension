{
  "manifest_version": 3,
  "name": "Multi-Language URL Opener",
  "version": "1.0.0",
  "description": "Open multiple URL variants with different language codes in separate tabs",
  "permissions": ["tabs", "storage"],

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Language Variants",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
